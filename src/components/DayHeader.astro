---
// DayHeader.astro: Table header cell for a day
export interface Props {
  name: string;
}
const { name } = Astro.props;
---
<div class="cell day-header">{name}</div>
<style>
  .day-header {
    background: #f1f1f1;
    font-weight: bold;
    align-items: center;
    justify-content: center;
    color: #444;
  }
</style>
